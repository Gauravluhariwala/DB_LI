# Production Environment Variables Template
# Copy this to .env and fill in your actual values

# OpenSearch Configuration
OPENSEARCH_ENDPOINT=il674y001legt8k99rt0.us-east-1.aoss.amazonaws.com
AWS_REGION=us-east-1

# AWS Credentials (for local development)
# In production Lambda, these come from IAM role
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here

# Index Names
COMPANIES_INDEX=linkedin-prod-companies
PROFILES_INDEX=linkedin_profiles_enriched_*

# Query Limits
MAX_COMPANIES_FILTER=200
DEFAULT_PAGE_SIZE=25
MAX_PAGE_SIZE=50

# API Configuration
API_TITLE=LinkedIn Sequential Search API
API_VERSION=1.0.0

# Session Token Secret (change in production!)
SESSION_TOKEN_SECRET=your-secret-key-change-in-production-use-strong-random-value
